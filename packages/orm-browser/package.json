{
  "name": "@deepkit/orm-browser",
  "version": "1.0.12",
  "description": "ORM Browser",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "peerDependencies": {
    "@deepkit/app": "workspace:*",
    "@deepkit/broker": "workspace:*",
    "@deepkit/bson": "workspace:*",
    "@deepkit/core": "workspace:*",
    "@deepkit/framework": "workspace:*",
    "@deepkit/http": "workspace:*",
    "@deepkit/injector": "workspace:*",
    "@deepkit/logger": "workspace:*",
    "@deepkit/orm": "workspace:*",
    "@deepkit/rpc": "workspace:*",
    "@deepkit/type": "workspace:*"
  },
  "dependencies": {
    "@deepkit/orm-browser-gui": "workspace:*"
  },
  "devDependencies": {
    "@deepkit/app": "workspace:*",
    "@deepkit/broker": "workspace:*",
    "@deepkit/bson": "workspace:*",
    "@deepkit/core": "workspace:*",
    "@deepkit/framework": "workspace:*",
    "@deepkit/http": "workspace:*",
    "@deepkit/injector": "workspace:*",
    "@deepkit/logger": "workspace:*",
    "@deepkit/orm": "workspace:*",
    "@deepkit/rpc": "workspace:*",
    "@deepkit/type": "workspace:*",
    "@types/node": "^22.13.1",
    "rxjs": "~7.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}

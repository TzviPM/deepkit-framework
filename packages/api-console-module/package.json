{
  "name": "@deepkit/api-console-module",
  "version": "1.0.12",
  "description": "API Console",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "peerDependencies": {
    "@deepkit/app": "workspace:*",
    "@deepkit/broker": "workspace:*",
    "@deepkit/bson": "workspace:*",
    "@deepkit/core": "workspace:*",
    "@deepkit/http": "workspace:*",
    "@deepkit/injector": "workspace:*",
    "@deepkit/logger": "workspace:*",
    "@deepkit/rpc": "workspace:*",
    "@deepkit/type": "workspace:*"
  },
  "dependencies": {
    "@deepkit/api-console-api": "workspace:*",
    "@deepkit/api-console-gui": "workspace:*"
  },
  "devDependencies": {
    "@deepkit/app": "workspace:*",
    "@deepkit/broker": "workspace:*",
    "@deepkit/bson": "workspace:*",
    "@deepkit/core": "workspace:*",
    "@deepkit/http": "workspace:*",
    "@deepkit/injector": "workspace:*",
    "@deepkit/logger": "workspace:*",
    "@deepkit/rpc": "workspace:*",
    "@deepkit/type": "workspace:*",
    "rxjs": "~7.8.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/tsconfig.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}

{
  "name": "@deepkit/orm-browser-example",
  "version": "1.0.1-alpha.14",
  "description": "Deepkit ORM Data Browser example",
  "type": "commonjs",
  "private": true,
  "main": "./dist/cjs/main.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
    "@deepkit/mongo": "workspace:*",
    "@deepkit/orm-browser": "workspace:*",
    "@deepkit/mysql": "workspace:*",
    "@deepkit/orm": "workspace:*",
    "@deepkit/postgres": "workspace:*",
    "@deepkit/sqlite": "workspace:*",
    "@deepkit/type": "workspace:*"
  },
  "devDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}
